@model IEnumerable<onlineKredit.web.Models.ZusammenfassungModel>

<div id="content">

    <div class="row">
        Werden auch Sie reicher als reich mit einem Kredit der Deutschen Bank AG!
    </div>
    @foreach (var item in Model)
    {
        <div class="row">
            <div class="col-lg-offset-1 col-lg-8">
                @item.Vorname.Substring(0, 1).@item.Nachname genießt jetzt @item.GewünschterBetrag €
            </div>
        </div>
    }

</div>

